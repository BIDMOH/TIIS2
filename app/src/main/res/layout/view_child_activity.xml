<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent" android:layout_height="fill_parent"
    android:fillViewport="true">
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background1"
    android:orientation="vertical"
    android:weightSum="1">


    <LinearLayout
        android:id="@+id/titleBarLayout"
        style="@style/TitleBar">

        <ImageButton
            android:id="@+id/img_btn_home_view_child"
            style="@style/TitleBarAction"
            android:contentDescription="@string/description_home"
            android:src="@drawable/title_home" />

        <TextView
            style="@style/TitleBarText"
            android:gravity="center"
            android:text="@string/title_view_child"
            android:textAlignment="center"
            android:textColor="@android:color/black"
            android:textSize="@dimen/text_size_large" />

        <ImageView
            android:id="@+id/home_start_continuous_sync"
            android:layout_width="@dimen/fifty_dp_height"
            android:layout_height="@dimen/forty_dp_height"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            android:layout_marginEnd="@dimen/ten_dp_height"
            android:layout_marginRight="@dimen/ten_dp_width"
            android:src="@drawable/ic_continuous_sync"
            android:onClick="onClickContinuousSync"/>

        <ImageView
            android:id="@+id/home_wifi_icon"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:background="#ffff1313"
            android:src="@drawable/wifi32" />

    </LinearLayout>



    <TableLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="20dp">

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="60dp">

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/view_child_barcode_id_str"
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/barcode_id" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/view_child_barcode_id"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"

                    android:background="@drawable/border_radius_text_box"
                    android:clickable="true"
                    android:digits="0123456789"
                    android:enabled="true"
                    android:focusableInTouchMode="true"
                    android:inputType="number"
                    android:textColor="@color/txt"
                    android:textSize="20dp"
                    android:textStyle="bold" />
                <!--android:background="@drawable/border_radius_text_box"-->

            </LinearLayout>

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textTemp"
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/temp_id" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lin_tempId"
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/view_child_temp_id"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border_radius_text_box"
                    android:clickable="true"
                    android:focusableInTouchMode="true"
                    android:inputType="number|textAutoComplete|"
                    android:numeric="integer"
                    android:textColor="@color/txt"
                    android:textStyle="bold" />

            </LinearLayout>

        </TableRow>


        <Space
            android:layout_width="10dp"
            android:layout_height="10dp" />


        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="60dp">

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/firstname" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/view_child_child_firstname"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border_radius_text_box"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                    android:inputType="textCapWords|textNoSuggestions"
                    android:lines="1"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:textColor="@color/txt" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/surname" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/view_child_child_lastname"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border_radius_text_box"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                    android:inputType="textCapWords|textNoSuggestions"
                    android:lines="1"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:textColor="@color/txt" />

            </LinearLayout>

        </TableRow>

        <Space
            android:layout_width="20dp"
            android:layout_height="10dp" />
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="60dp"
            android:orientation="horizontal">

            <TextView
                style="@style/LabelChild"
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:text="@string/firstname2" />

            <EditText
                android:id="@+id/firstname2"
                android:layout_width="170dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/border_radius_text_box"
                android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                android:inputType="textCapWords|textNoSuggestions"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/txt" />

        </LinearLayout>

        <Space
            android:layout_width="20dp"
            android:layout_height="20dp" />
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="60dp">

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/mother_firstname" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/view_child_mother_firstname"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border_radius_text_box"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                    android:inputType="textCapWords|textNoSuggestions"
                    android:lines="1"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:textColor="@color/txt" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/mother_surname" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/view_child_mother_lastname"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border_radius_text_box"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                    android:inputType="textCapWords|textNoSuggestions"
                    android:lines="1"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:textColor="@color/txt" />

            </LinearLayout>

        </TableRow>

        <Space
            android:layout_width="20dp"
            android:layout_height="10dp" />


        <!--DateofBirth and Gender-->
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="60dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/date_of_birth" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <Button
                    android:id="@+id/view_child_dob"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border_radius_text_box"
                    android:drawableEnd="@drawable/calendar_ico"
                    android:drawableRight="@drawable/calendar_ico"
                    android:gravity="left|center_vertical"
                    android:hint="@string/date_of_birth"
                    android:textColor="@color/txt"
                    android:textSize="18sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/gender" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <RadioGroup
                    android:id="@+id/view_child_gender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:orientation="horizontal">

                    <RadioButton
                        android:id="@+id/male_radio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"
                        android:background="@drawable/border_check_radio"
                        android:checked="false" />

                    <TextView
                        style="@style/LabelChild"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"
                        android:text="@string/radio_male" />

                    <RadioButton
                        android:id="@+id/female_radio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"
                        android:background="@drawable/border_check_radio"
                        android:checked="false" />

                    <TextView
                        style="@style/LabelChild"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"
                        android:text="@string/radio_female" />


                </RadioGroup>


            </LinearLayout>

        </TableRow>

        <Space
            android:layout_width="20dp"
            android:layout_height="10dp" />

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="60dp">

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/place_of_birth" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <Spinner
                    android:id="@+id/view_child_birthplace"
                    style="@style/spinner_style"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:drawSelectorOnTop="true"
                    android:gravity="left"
                    android:spinnerMode="dialog" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/village_domicile" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <Spinner
                    android:id="@+id/view_child_village"
                    style="@style/spinner_style"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:spinnerMode="dialog"
                    android:textColor="@color/txt" />

            </LinearLayout>

        </TableRow>

        <Space
            android:layout_width="20dp"
            android:layout_height="10dp" />

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="60dp">

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/health_facility" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <Spinner
                    android:id="@+id/view_child_health_facility"
                    style="@style/spinner_style"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:spinnerMode="dialog"
                    android:textColor="@color/txt" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/status" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <Spinner
                    android:id="@+id/view_child_status"
                    style="@style/spinner_style"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:spinnerMode="dialog"
                    android:textColor="@color/txt" />

            </LinearLayout>

        </TableRow>

        <Space
            android:layout_width="20dp"
            android:layout_height="10dp" />

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="60dp">

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/phone" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/view_child_phone"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border_radius_text_box"
                    android:digits="0123456789"
                    android:inputType="phone"
                    android:textColor="@color/txt" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="60dp"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"
                android:orientation="vertical">

                <TextView
                    style="@style/LabelChild"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:text="@string/notes" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="170dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/view_child_notes"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/border_radius_text_box"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789 "
                    android:inputType="textCapWords|textNoSuggestions"
                    android:textColor="@color/txt" />

            </LinearLayout>

        </TableRow>

    </TableLayout>


    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp">

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="60dp">

            <LinearLayout
                android:layout_width="250dp"
                android:layout_height="60dp"
                android:layout_marginEnd="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginStart="20dp"
                android:orientation="vertical">

                <Button
                    android:id="@+id/back_btn"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:background="@drawable/border_radius_button"
                    android:text="@string/back"
                    android:textSize="@dimen/text_size_large" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:orientation="vertical">

                <Button
                    android:id="@+id/view_child_save_button"
                    android:layout_width="150dp"
                    android:layout_height="50dp"
                    android:background="@drawable/border_radius_button"
                    android:ellipsize="middle"
                    android:onClick="onClickFeature"
                    android:text="@string/btn_save"
                    android:textSize="@dimen/text_size_large" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="300dp"
                android:layout_height="60dp"
                android:orientation="vertical">

                <Button
                    android:id="@+id/home_btn_scan"
                    android:layout_width="150dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginStart="0dp"
                    android:background="@drawable/scan_button_color"
                    android:ellipsize="middle"
                    android:text="@string/btn_scan"
                    android:textSize="@dimen/text_size_large" />
            </LinearLayout>
        </TableRow>
    </TableLayout>

    <!--Table Header-->
    <TableLayout
        android:id="@+id/view_appointment_table_layout_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:paddingTop="50dp"
        android:shrinkColumns="*"
        android:stretchColumns="*">
        <!--Table Header-->
        <TableRow
            android:id="@+id/view_appointment_table_header"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#73819fd1"
            android:padding="15dp">

            <TextView
                android:gravity="center_horizontal"
                android:text="@string/vaccine_dose"
                android:textColor="@color/txt"
                android:textSize="@dimen/text_size_medium" />

            <TextView
                android:gravity="center_horizontal"
                android:text="@string/schedule"
                android:textColor="@color/txt"
                android:textSize="@dimen/text_size_medium" />

            <TextView
                android:gravity="center_horizontal"
                android:text="@string/scheduled_date"
                android:textColor="@color/txt"
                android:textSize="@dimen/text_size_medium" />
        </TableRow>
    </TableLayout>

    <!--Table-->
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="200dp">

    <TableLayout
        android:id="@+id/view_appointment_table_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:shrinkColumns="*"
        android:stretchColumns="*">


    </TableLayout>
    </ScrollView>

    <!--Buttons-->
    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="60dp">

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal">

            <LinearLayout
